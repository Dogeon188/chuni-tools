{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 90,
	"semi": false,
	"tabWidth": 4,
	"svelteBracketNewLine": false,
	"svelteSortOrder": "options-scripts-markup-styles",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"tailwindStylesheet": "./src/app.css"
}
