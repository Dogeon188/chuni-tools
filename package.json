{
	"name": "chuni-tools",
	"version": "2.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:app": "vite dev",
		"build": "npm run build:scripts && npm run build:app",
		"build:dev": "ENV=development npm run build:scripts && npm run build:app",
		"build:app": "vite build",
		"build:scripts": "vite build --config vite.scripts.config.ts",
		"build:scripts:dev": "ENV=development vite build --config vite.scripts.config.ts",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.32.0",
		"@playwright/test": "^1.54.1",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.26.1",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@tailwindcss/vite": "^4.1.11",
		"@types/markdown-it": "^14.1.2",
		"@types/node": "^24.1.0",
		"@types/prismjs": "^1.26.5",
		"@vitejs/plugin-basic-ssl": "^2.1.0",
		"@vitest/browser": "^3.2.4",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"glob": "^11.0.3",
		"globals": "^16.3.0",
		"mdsvex": "^0.12.6",
		"playwright": "^1.54.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.37.1",
		"svelte-check": "^4.3.0",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0",
		"vite": "^7.0.6",
		"vite-plugin-mkcert": "^1.17.8",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.2.0",
		"@types/markdown-it-container": "^2.0.10",
		"html-to-image": "^1.11.13",
		"markdown-it": "^14.1.0",
		"markdown-it-container": "^4.0.0",
		"material-icons": "^1.13.14"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "4.46.2"
	},
	"overrides": {
		"vite": {
			"rollup": "npm:@rollup/wasm-node"
		}
	},
	"trustedDependencies": [
		"@tailwindcss/oxide"
	]
}
